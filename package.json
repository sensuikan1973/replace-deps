{
  "name": "replace-deps",
  "version": "2.0.0",
  "description": "replace dependencies in specific extenstion files.",
  "main": "lib/main.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "all": "npm run build && npm run format && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensuikan1973/replace-deps.git"
  },
  "keywords": [
    "actions",
    "html",
    "javascript",
    "firebasejs",
    "firebase"
  ],
  "author": "sensuikan1973",
  "bugs": {
    "url": "https://github.com/sensuikan1973/replace-deps/issues"
  },
  "homepage": "https://github.com/sensuikan1973/replace-deps#readme",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "glob": "^8.0.3"
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@zeit/ncc": "^0.22.3",
    "nyc": "^15.1.0",
    "prettier": "2.6.2",
    "typescript": "^4.7.3"
  }
}
