{
  "name": "replace-deps",
  "version": "2.1.0",
  "description": "replace dependencies in specific extenstion files.",
  "main": "lib/main.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "pack": "ncc build",
    "all": "npm run build && npm run format && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sensuikan1973/replace-deps.git"
  },
  "keywords": [
    "actions",
    "html",
    "javascript",
    "firebasejs",
    "firebase"
  ],
  "author": "sensuikan1973",
  "bugs": {
    "url": "https://github.com/sensuikan1973/replace-deps/issues"
  },
  "homepage": "https://github.com/sensuikan1973/replace-deps#readme",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.1",
    "glob": "^11.0.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@zeit/ncc": "^0.22.3",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
